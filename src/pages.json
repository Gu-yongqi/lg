        {
            "root": "pages/login",
            "name": "login",
            "pages": [{
                    "path": "login",
                    "style": {
                        "app-plus": {
                            "titleNView": false
                        },
                        "disableScroll": true,
                        "navigationBarTitleText": "登录",
