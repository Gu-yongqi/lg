    "tabBar": {
        "selectedColor": "#e22519",
        "fontSize": "10px",
        "borderStyle": "black",
        "list": [{
                "pagePath": "pages/home/home",
                "text": "首页",
                "iconPath": "static/img/home@2x.png",
                "selectedIconPath": "static/img/home-active@2x.png"
            },
            {
                "pagePath": "pages/shop/shop",
                "text": "选品库",
                "iconPath": "static/img/stock@2x.png",
